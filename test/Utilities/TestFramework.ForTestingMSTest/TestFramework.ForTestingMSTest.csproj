<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Nullable>enable</Nullable>
    <!-- Assembly name must end with TestAdapter to be picked up by Test Platform -->
    <AssemblyName>TestFramework.ForTestingMSTest.TestAdapter</AssemblyName>
    <RootNamespace>TestFramework.ForTestingMSTest</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.TestPlatform.ObjectModel" />
  </ItemGroup>

</Project>
